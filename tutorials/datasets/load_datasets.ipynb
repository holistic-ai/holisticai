    "import pandas as pd\n",
    "from holisticai.datasets import Dataset\n",
    "\n",
    "X = pd.DataFrame(iris['data'], columns=iris['feature_names'])\n",
    "y = pd.Series(iris['target'], name='target')\n",
    "\n",
    "ds = Dataset(X=X, y=y)\n",
    "ds"
